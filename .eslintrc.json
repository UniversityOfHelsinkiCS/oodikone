{
    "root": true,
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "comma-dangle": [2, "never"],
      "no-param-reassign": [2, {
        "props": false
      }],
      "import/no-unresolved":2,
      "react/jsx-no-bind": 2,
      "react/no-did-mount-set-state": 0,
      "react/prefer-stateless-function": 2,
      "react/no-typos": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/click-events-have-key-events": 0
    },
    "plugins" : [
      "react",
      "import"
    ]
}
