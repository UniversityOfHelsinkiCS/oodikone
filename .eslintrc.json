{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:cypress/recommended"
  ],
  "plugins": [
    "prettier",
    "cypress"
  ],
  "rules": {
    "no-console": "off",
    "react/no-unescaped-entities": "off",
    "prettier/prettier": "error"
  }
}
