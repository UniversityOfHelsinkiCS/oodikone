{
  "name": "oodikone-backend",
  "version": "0.0.1",
  "description": "Backend for Oodikone",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "docker_local": "NODE_ENV=docker_local nodemon index.js",
    "dev": "NODE_ENV=dev nodemon index.js",
    "test": "NODE_ENV=test nyc ava --serial",
    "supertest": "NODE_ENV=test ava test/supertests/*.js",
    "update_database_dev": "NODE_ENV=dev node src/services/database_updater/database_updater.js",
    "update_database": "node src/services/database_updater/database_updater.js",
    "update_studentnumbers_dev": "NODE_ENV=dev node src/services/database_updater/student_list_updater.js",
    "lint": "eslint --parser babel-eslint src",
    "coverage": "NODE_ENV=test nyc ava"
  },
  "author": "Matti Luukkainen",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-basic-auth": "^1.1.2",
    "express-session": "^1.15.6",
    "install": "^0.10.1",
    "jsonwebtoken": "^8.1.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.5",
    "moment": "^2.19.1",
    "npm": "^5.4.2",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "redis": "^2.8.0",
    "sequelize": "^4.35.1",
    "umzug": "^2.1.0",
    "winston": "^2.4.1",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-eslint": "^8.0.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.9.0",
    "faker": "^4.1.0",
    "nodemon": "^1.17.1",
    "nyc": "^11.6.0",
    "supertest": "^3.0.0"
  },
  "ava": {
    "files": [
      "test/**/*_test.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!test/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
