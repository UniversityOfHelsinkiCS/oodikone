{
  "name": "oodikone-backend",
  "version": "0.0.1",
  "description": "Backend for Oodikone",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "docker_local": "NODE_ENV=docker_local nodemon index.js",
    "dev": "NODE_ENV=dev nodemon index.js",
    "test": "NODE_ENV=test nyc ava",
    "supertest": "NODE_ENV=test ava test/supertests/*.js",
    "update_database_dev": "NODE_ENV=dev node src/services/database_updater/database_updater.js",
    "update_database": "node src/services/database_updater/database_updater.js",
    "update_studentnumbers_dev": "NODE_ENV=dev node src/services/database_updater/student_list_updater.js",
    "init_redis": "node src/util/init_redis.js",
    "seed_redis": "node src/util/add_json_duplicates_to_redis.js && node src/util/map_course_codes.js",
    "studentnumbers_to_file": "NODE_ENV=dev node src/services/doo_api_database_updater/db_studentnumbers_to_file.js",
    "update_database_dev_new_api": "NODE_ENV=dev node src/services/doo_api_database_updater/database_updater.js",
    "lint": "eslint --parser babel-eslint src",
    "coverage": "NODE_ENV=test nyc ava"
  },
  "author": "Matti Luukkainen",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "raven": "^2.5.0",
    "redis": "^2.8.0",
    "sequelize": "^4.37.6",
    "umzug": "^2.1.0",
    "uuid": "^3.2.1",
    "winston": "^2.4.1",
    "winston-log2gelf": "^1.6.0",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-eslint": "^8.2.3",
    "dotenv": "^4.0.0",
    "eslint": "^4.19.1",
    "faker": "^4.1.0",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "supertest": "^3.0.0"
  },
  "ava": {
    "files": [
      "test/**/*_test.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!test/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
