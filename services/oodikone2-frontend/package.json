    "stats": "webpack --json --mode=production --profile --progress > stats.json",
    "test": "npm run lint",
    "test_docker": "docker exec -t frontend /bin/bash -c 'npm test'",
    "notest": "echo",
    "storybook": "start-storybook -h 0.0.0.0 -p 7777 --ci",
    "eslint-check": "eslint --print-config src/app.jsx | eslint-config-prettier-check"
  },
